{
  "name": "awsdevhour",
  "version": "0.1.1",
  "bin": {
    "awsdevhour": "bin/awsdevhour.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
