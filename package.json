  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "20.9.5",
    "aws-cdk": "^2.110.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.3.2"
  },
  "dependencies": {
    "constructs": "^10.0.0"
  }
}
